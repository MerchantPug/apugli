{
  "type": "apoli:multiple",
  "resource": {
    "type": "apoli:resource",
    "min": 0,
    "max": 6,
    "hud_render": {
      "should_render": false
    },
    "max_action": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "apoli:spawn_particles",
          "particle": "minecraft:cloud",
          "count": 8,
          "speed": 0.5,
          "offset_y": 0.0
        },
        {
          "type": "apoli:execute_command",
          "command": "playsound apugli:origin.hare.max_speed player @a ~ ~ ~ 1.0 1.0"
        }
      ]
    }
  },
  "cooldown": {
    "type": "apoli:cooldown",
    "cooldown": 10,
    "hud_render": {
      "should_render": false
    }
  },
  "jump_action": {
    "type": "apugli:action_on_jump",
    "entity_action": {
      "type": "apoli:change_resource",
      "resource": "*:*_resource",
      "change": 1
    },
    "condition": {
      "type": "apoli:or",
      "conditions": [
        {
          "type": "apoli:sprinting"
        },
        {
          "type": "apoli:and",
          "conditions": [
            {
              "type": "apoli:fluid_height",
              "fluid": "minecraft:water",
              "comparison": ">",
              "compare_to": 0.0
            },
            {
              "type": "apoli:resource",
              "resource": "*:*_resource",
              "comparison": "==",
              "compare_to": 6
            }
          ]
        }
      ]
    }
  },
  "air_speed": {
    "type": "apoli:modify_air_speed",
    "modifier": {
      "operation": "add_base_early",
      "value": 0.00125,
      "modifier": {
        "name": "Bonus air speed from stored momentum",
        "operation": "multiply_total_multiplicative",
        "value": 0.0,
        "resource": "*:*_resource"
      }
    }
  },
  "fov": {
    "type": "apugli:modify_fov",
    "modifier": {
      "operation": "multiply_base_multiplicative",
      "value": 0.025,
      "modifier": {
        "name": "Additional FOV from stored momentum",
        "operation": "multiply_total_multiplicative",
        "value": 0.0,
        "resource": "*:*_resource"
      }
    },
    "change_divisor": 1.25,
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": ">",
      "compare_to": 0
    }
  },
  "extra_max_fov": {
    "type": "apugli:modify_fov",
    "modifier": {
      "name": "Additional FOV from maximum momentum",
      "operation": "multiply_total_multiplicative",
      "value": 0.035
    },
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": "==",
      "compare_to": 6
    }
  },
  "reset": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:if_else",
      "condition": {
        "type": "apoli:or",
        "conditions": [
          {
            "type": "apoli:fluid_height",
            "fluid": "minecraft:water",
            "comparison": ">",
            "compare_to": 0.4
          },
          {
            "type": "apugli:grounded"
          }
        ]
      },
      "if_action": {
        "type": "apoli:if_else",
        "condition": {
          "type": "apoli:or",
          "conditions": [
            {
              "type": "apoli:fluid_height",
              "fluid": "minecraft:water",
              "comparison": ">",
              "compare_to": 0.4
            },
            {
              "type": "apoli:resource",
              "resource": "*:*_cooldown",
              "comparison": "==",
              "compare_to": 0
            }
          ]
        },
        "if_action": {
          "type": "apoli:change_resource",
          "resource": "*:*_resource",
          "change": 0,
          "operation": "SET"
        }
      },
      "else_action": {
        "type": "apoli:trigger_cooldown",
        "power": "*:*_cooldown"
      }
    },
    "interval": 1,
    "condition": {
      "type": "apoli:or",
      "conditions": [
        {
          "type": "apoli:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.4
        },
        {
          "type": "apoli:and",
          "conditions": [
            {
              "type": "apoli:resource",
              "resource": "*:*_resource",
              "comparison": ">",
              "compare_to": 0
            }
          ]
        }
      ]
    }
  }
}